{
  "name": "greg36/simply-filters",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress"
  ],
  "description": "Simply add product filters to WooCommerce.",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Grzegorz Niedzielski",
      "email": "admin@gregn.pl"
    }
  ],
  "autoload": {
    "psr-4": {
      "SimplyFilters\\": "app/"
    }
  },
  "require": {
    "php": ">=7.0",
    "themehybrid/hybrid-core": "^6.0"
  },
  "require-dev": {
    "automattic/vipwpcs": "^2.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
